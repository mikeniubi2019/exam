<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xueyi.exam.mapper.PageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xueyi.exam.beans.Page">
        <id column="id" property="id" />
        <result column="course_id" property="courseId" />
        <result column="student_count" property="studentCount" />
        <result column="question_count" property="questionCount" />
        <result column="max_score" property="maxScore" />
        <result column="page_name" property="pageName" />
        <result column="page_description" property="pageDescription" />
        <result column="creat_time" property="creatTime" />
        <result column="page_teacher" property="pageTeacher" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, course_id, student_count, question_count, max_score, page_name, page_description, creat_time, page_teacher
    </sql>

</mapper>
